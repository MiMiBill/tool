<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--自定义seekbarstyle-->
    <style name="CustomSeekbarStyle" >
        <item name="android:maxHeight">11dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@color/color_f98248</item>
        <item name="android:progressDrawable">@drawable/seekbar_bg</item>
        <item name="android:minHeight">11dp</item>
        <item name="android:thumb">@drawable/test_point</item>
    </style>


</resources>
